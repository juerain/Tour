a() {\r\n    return {\r\n      imglist: [],\r\n      uid:\"\"\r\n    };\r\n  },\r\n  props:[\"uid\"],\r\n  // 接收父组件传来的数据\r\n  props: {\r\n    \r\n    imgs: { default: \"\" },\r\n    // 左侧图片\r\n    leftImg: { default: \"\" },\r\n    // 左侧文字\r\n    leftTitle: { default: \"\" },\r\n    // 右侧图片\r\n    rightImg: { default: \"\" },\r\n    // 右侧文字\r\n    rightTitle: { default: \"\" },\r\n    addfiend: { default: \"\" }\r\n  },\r\n  methods: {\r\n    house(){\r\n      this.$router.push(\"houseandhome\");\r\n    },\r\n    build(){\r\n      this.$router.push(\"build\");\r\n    },\r\n    food(){\r\n      this.$router.push(\"food\");\r\n    },\r\n    igtv(){\r\n      this.$router.push(\"igtv\");\r\n    },\r\n    addFiend(e) {\r\n      var addImg = document.getElementById(\"addImg\");\r\n      if (e.target == addImg) {\r\n        this.$router.push(\"/SuggestListAllMsg\");\r\n      }\r\n    },\r\n    search(e) {\r\n      var searchImg = document.getElementById(\"searchImg\");\r\n      if (e.target == searchImg) {\r\n        this.$router.push(\"/hotsearch\");\r\n      }\r\n    },\r\n    load() {\r\n      \r\n      this.axios(\"cy\").then(res => {\r\n        // console.log(res);\r\n        this.imglist = res.data.data.result1.slice(28,37);\r\n        console.log(this.uid);\r\n        // console.log(this.imglist[0]);\r\n      });\r\n    },\r\n    \r\n  },\r\n  created() {\r\n    this.load();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 外层容器样式 */\r\ndiv.search_head_box {\r\n  width: 100%;\r\n  height: 100px;\r\n  background: #eee;\r\n}\r\n\r\ndiv.search_head {\r\n  padding: 0;\r\n  margin: 0;\r\n  width: 100%;\r\n  height: 50px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  /* position: fixed; */\r\n  padding-left: 7px;\r\n  padding-right: 7px;\r\n}\r\n/* 内层容器样式 */\r\ndiv.left_head,\r\ndiv.right_head {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n/* 图片样式 */\r\ndiv.search_head img {\r\n  width: 25px;\r\n  display: inline-block;\r\n}\r\n/* 加号样式 */\r\ndiv.search_head > div.right_head > span {\r\n  font-size: 20px;\r\n}\r\n/* 搜索字体样式 */\r\ndiv.search_head > div.left_head > span {\r\n  font-size: 20px;\r\n  display: inline-block;\r\n  margin: 10px 10px;\r\n}\r\n/* 下部按钮样式 */\r\ndiv.btn {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  width: 100%;\r\n}\r\n\r\n/* 外层容器样式 */\r\ndiv.imgs_list{\r\n  \r\n}\r\ndiv.search_img {\r\n  width: 100%;\r\n  display: inline;\r\n}\r\n/* 图片样式 */\r\ndiv.search_img img {\r\n  width: 117px;\r\n  height: 100px;\r\n  margin-left: 2px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\components\\ch\\common\\second-search"}]}