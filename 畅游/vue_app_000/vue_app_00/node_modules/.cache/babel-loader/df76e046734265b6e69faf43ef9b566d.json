  food(){\r\n      this.$router.push(\"food\");\r\n    },\r\n    igtv(){\r\n      this.$router.push(\"igtv\");\r\n    },\r\n    search(e) {\r\n      var searchImg = document.getElementById(\"searchImg\");\r\n      if (e.target == searchImg) {\r\n        this.$router.push(\"/hotsearch\");\r\n      }\r\n    },\r\n    load() {\r\n      this.axios(\"cy\").then(res => {\r\n        // console.log(res);\r\n        this.imglist = res.data.data.result1.slice(18,27);\r\n        // console.log(imglist);\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    this.load();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 外层容器样式 */\r\ndiv.search_head_box {\r\n  width: 100%;\r\n  height: 100px;\r\n  background: #eee;\r\n}\r\n/* 搜索框样式 */\r\n  .search_input{\r\n    width: 200px;\r\n    margin-left: 20px;\r\n    height: 25px;\r\n    border: 0px;\r\n    padding-left: 5px;\r\n  }\r\ndiv.search_head {\r\n  padding: 0;\r\n  margin: 0;\r\n  width: 100%;\r\n  height: 50px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  /* position: fixed; */\r\n  padding-left: 7px;\r\n  padding-right: 7px;\r\n}\r\n/* 内层容器样式 */\r\ndiv.left_head,\r\ndiv.right_head {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\ndiv.left_head span{\r\n  font-weight: bolder;\r\n}\r\n#addImg{\r\n  margin-top: 5px;\r\n}\r\n/* 图片样式 */\r\ndiv.search_head img {\r\n  width: 25px;\r\n  display: inline-block;\r\n}\r\n/* 加号样式 */\r\ndiv.search_head > div.right_head > span {\r\n  font-size: 20px;\r\n}\r\n/* 搜索字体样式 */\r\ndiv.search_head > div.left_head > span {\r\n  font-size: 20px;\r\n  display: inline-block;\r\n  margin: 10px 10px;\r\n}\r\n/* 下部按钮样式 */\r\ndiv.btn {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  width: 100%;\r\n}\r\n\r\n/* 外层容器样式 */\r\ndiv.search_img {\r\n  width: 100%;\r\n  display: inline;\r\n}\r\n/* 图片样式 */\r\ndiv.search_img img {\r\n  width: 117px;\r\n  height: 100px;\r\n  margin-left: 2px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\components\\ch\\common\\second-search"}]}