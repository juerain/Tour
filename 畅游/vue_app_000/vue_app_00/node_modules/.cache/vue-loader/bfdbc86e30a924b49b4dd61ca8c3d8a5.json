"file":"TvVideo-2.vue","sourceRoot":"src\\components\\ch\\common\\First-home","sourcesContent":["<template>\r\n    <div id=\"tvvideo-2\">\r\n        <div id=\"tvvideo-2-img\">\r\n            <div @click=\"tvvideoquit\" v-show=\"loaddown\" id=\"tvvideo-2-msg\">\r\n                <img src=\"../../../../img/tvvideoquit-icon.png\" alt=\"\">\r\n            </div>\r\n            <div @click=\"tvvideoimg\" v-for=\"(item,i) of list\" :key=\"i\">\r\n                <img class=\"tvvideo2-img\" :src=\"'http://127.0.0.1:3000/' +item.uimgurl\" alt=\"\" />\r\n                <img @click=\"tvvideoperson\" :data-did=\"item.did\" class=\"tvvideo-headimg\" :src=\"'http://127.0.0.1:3000/' +item.uheadurl\" alt=\"\" />\r\n            </div>\r\n            \r\n        </div>\r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            list:[],\r\n            loaddown:false,\r\n        }\r\n    },\r\n    methods: {\r\n        loadmore(){\r\n            // 获取地址栏上的传参值obj\r\n            var obj=this.$route.params;\r\n            // console.log(obj);\r\n            //请求服务器网址\r\n            var url=\"tvvideo2\";\r\n            //发送ajax请求获取数据\r\n            this.axios.get(url,{params:obj}).then(result=>{\r\n                var list=result.data.data;\r\n                this.list=list;\r\n            })\r\n           \r\n        },\r\n        tvvideoimg(){\r\n            this.loaddown=!this.loaddown\r\n        },\r\n        tvvideoquit(){\r\n            this.$router.push(\"/Tv\");\r\n        },\r\n        tvvideoperson(e){\r\n            var did=e.target.dataset.did;\r\n            this.$router.push(\"/TvVideo1/\"+did);\r\n        }\r\n       \r\n    }, \r\n    created(){//页面加载自动执行loadmore()\r\n            this.loadmore();\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n    #tvvideo-2-img{\r\n        position: relative;\r\n        height:653px;\r\n    }\r\n   \r\n    #tvvideo-2-msg{\r\n        position: absolute;\r\n        z-index: 5;\r\n        right:10px;\r\n        top:10px;\r\n    } \r\n    .tvvideo2-img{\r\n        position: absolute;\r\n        width:100%;\r\n        height:653px;\r\n        z-index: 1;\r\n    }\r\n    .tvvideo-headimg{\r\n        position: absolute;\r\n        top:10px;\r\n        left:10px;\r\n        z-index: 2;\r\n        width:50px;\r\n        height:50px;\r\n        border-radius: 50%;\r\n    }\r\n</style>\r\n\r\n"]}]}